package TeacherGPT;

public class R02_null {
    public static void main(String[] args) {

        String str1;
        String str2;

        str1 = "Hello";
        str2 = null;

        if (str1 == null){
            System.out.println("Null variable");
        }

        if (str2 == null){
            System.out.println("Null variable");
        }
        
    }
}
